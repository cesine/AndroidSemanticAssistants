<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
<!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
<!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
<!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
]>

<rdf:RDF
  xmlns     = "http://lod.semanticsoftware.info/pubo/pubo#"
  xml:base  = "http://lod.semanticsoftware.info/pubo/pubo#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:rdf = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs= "http://www.w3.org/2000/01/rdf-schema#"
  xmlns:xsd = "http://www.w3.org/2001/XMLSchema#"
  xmlns:sro="http://salt.semanticauthoring.org/ontologies/sro#"
  xmlns:sdo="http://salt.semanticauthoring.org/ontologies/sdo#"
  xmlns:swc="http://data.semanticweb.org/ns/swc/ontology#">

<rdf:Property rdf:about="containsNE"
    <rdfs:label xml:lang="en">containsNE</rdfs:label>
    <rdfs:comment>The relation between a sentence and a (linked) named entity.</rdfs:comment>
    <rdfs:domain rdf:resource="sro:RhetoricalElement"/>
    <rdfs:range rdf:resource="pubo:LinkedNamedEntity"/>
</rdf:Property>

<rdf:Property rdf:about="correspondingAuhtor">
    <rdfs:label xml:lang="en">correspondingAuthor</rdfs:label>
    <rdfs:comment>The corresponding author for the publication.</rdfs:comment>
    <rdfs:domain rdf:resource="swc:Publication"/>
    <rdfs:range rdf:resource="foaf:Person"/>
</rdf:Property>

</rdf:RDF>

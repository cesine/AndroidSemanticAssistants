<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://localhost/SemanticAssistants.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:cu="http://localhost/ConceptUpper.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://localhost/SemanticAssistants.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://localhost/ConceptUpper.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="GATECorpus">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="GATECorpusFormat"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://localhost/ConceptUpper.owl#hasFormat"/>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://localhost/ConceptUpper.owl#Artifact"/>
  </owl:Class>
  <owl:Class rdf:ID="GATEAnnotation">
    <rdfs:subClassOf rdf:resource="http://localhost/ConceptUpper.owl#Artifact"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="GATEFormat"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://localhost/ConceptUpper.owl#hasFormat"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#GATEFormat">
    <rdfs:subClassOf rdf:resource="http://localhost/ConceptUpper.owl#Format"/>
  </owl:Class>
  <owl:Class rdf:ID="GATEPipelineFormat">
    <rdfs:subClassOf rdf:resource="#GATEFormat"/>
  </owl:Class>
  <owl:Class rdf:ID="GATEPipeline">
    <rdfs:subClassOf rdf:resource="http://localhost/ConceptUpper.owl#Tool"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#GATEPipelineFormat"/>
        <owl:onProperty rdf:resource="http://localhost/ConceptUpper.owl#hasFormat"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="GATE_OutputArtifact">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://localhost/ConceptUpper.owl#hasFormat"/>
            <owl:someValuesFrom rdf:resource="#GATEFormat"/>
          </owl:Restriction>
          <rdf:Description rdf:about="http://localhost/ConceptUpper.owl#IOArtifact"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#GATECorpusFormat">
    <rdfs:subClassOf rdf:resource="#GATEFormat"/>
  </owl:Class>
  <owl:Class rdf:ID="GATE_RTParameterFormat">
    <rdfs:subClassOf rdf:resource="#GATEFormat"/>
  </owl:Class>
  <owl:Class rdf:ID="IRTool">
    <rdfs:subClassOf rdf:resource="http://localhost/ConceptUpper.owl#Tool"/>
  </owl:Class>
  <owl:Class rdf:ID="GATEAnnotationFormat">
    <rdfs:subClassOf rdf:resource="#GATEFormat"/>
  </owl:Class>
  <owl:Class rdf:ID="NLPTool">
    <rdfs:subClassOf rdf:resource="http://localhost/ConceptUpper.owl#Tool"/>
  </owl:Class>
  <owl:Class rdf:ID="ParameterValuePair">
    <rdfs:subClassOf rdf:resource="http://localhost/ConceptUpper.owl#Artifact"/>
  </owl:Class>
  <owl:Class rdf:ID="GATERuntimeParameter">
    <rdfs:subClassOf rdf:resource="http://localhost/ConceptUpper.owl#Parameter"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasOutputNaturalLanguage">
    <rdfs:range rdf:resource="http://localhost/ConceptUpper.owl#NaturalLanguage"/>
    <rdfs:domain rdf:resource="#NLPTool"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Use "cu:anyLanguage" or leave empty if language independent</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="urlGivenByParameter">
    <rdfs:range rdf:resource="#GATERuntimeParameter"/>
    <rdfs:domain rdf:resource="#GATE_OutputArtifact"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Parameter receiving the file name of the output file. Only set this property if the output is actually a file. If this property is set, the parameter should not be made known to the end user.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasInputNaturalLanguage">
    <rdfs:range rdf:resource="http://localhost/ConceptUpper.owl#NaturalLanguage"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Use "cu:anyLanguage" or leave empty if language independent</rdfs:comment>
    <rdfs:domain rdf:resource="#NLPTool"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="necessaryParameterSetting">
    <rdfs:range rdf:resource="#ParameterValuePair"/>
    <rdfs:domain rdf:resource="#GATE_OutputArtifact"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Intended for output artifacts whose existence or format depends on certain input parameters and their values.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="giveInputToParameter">
    <rdfs:range rdf:resource="#GATERuntimeParameter"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Useful for query parameters, e.g., in the Yahoo search</rdfs:comment>
    <rdfs:domain rdf:resource="#GATEPipeline"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="parameter">
    <rdfs:domain rdf:resource="#ParameterValuePair"/>
    <rdfs:range rdf:resource="#GATERuntimeParameter"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="mergeInputDocuments">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#GATEPipeline"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Are input documents to be merged into one before they are passed to this language service?</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasGATEName">
    <rdfs:domain rdf:resource="http://localhost/ConceptUpper.owl#Artifact"/>
    <rdfs:subPropertyOf rdf:resource="http://localhost/ConceptUpper.owl#hasIdentifier"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isPerDocument">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#GATE_OutputArtifact"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >In case of an output artifact, is this artifact produced for each input document, or is it valid for a whole input corpus?</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="value">
    <rdfs:domain rdf:resource="#ParameterValuePair"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >For lists, use syntax (value1, value2, value3, ...)</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isBoundless">
    <rdfs:domain rdf:resource="#GATEAnnotation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="publishAsNLPService">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="http://localhost/ConceptUpper.owl#Tool"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasFeature">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >For GATEAnnotations. Which additional features of the annotation should be passed on to the client?</rdfs:comment>
    <rdfs:domain rdf:resource="#GATEAnnotation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="paramType">
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >string</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >int</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >url</rdf:first>
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >list(double)</rdf:first>
                      <rdf:rest rdf:parseType="Resource">
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        >list(int)</rdf:first>
                        <rdf:rest rdf:parseType="Resource">
                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >list(boolean)</rdf:first>
                          <rdf:rest rdf:parseType="Resource">
                            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >list(url)</rdf:first>
                          </rdf:rest>
                        </rdf:rest>
                      </rdf:rest>
                    </rdf:rest>
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >list(string)</rdf:first>
                  </rdf:rest>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >boolean</rdf:first>
              </rdf:rest>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >double</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The type of a parameter. Use string, double, int, or boolean for now.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#GATERuntimeParameter"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="defaultValue">
    <rdfs:domain rdf:resource="#GATERuntimeParameter"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="concatenationOfPipelines">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Comma-separated list of names (property hasGATEName) of pipelines</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#GATEPipeline"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="appFileName">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The actual filename of the GATE application, without path, e.g. Durm-Indexer</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#GATEPipeline"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="prName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The name of the Processing Resource (PR) the parameter belongs to. Available by pr.getName()</rdfs:comment>
    <rdfs:domain rdf:resource="#GATERuntimeParameter"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isInAnnotationSet">
    <rdfs:domain rdf:resource="#GATEAnnotation"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >For GATE annotations. In which annotation set are they located?</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <GATECorpusFormat rdf:ID="GATESerializedXMLCorpus">
    <cu:humanReadableFormat xml:lang="en">Serialized GATE XML Corpus</cu:humanReadableFormat>
    <cu:mimeType xml:lang="en">none</cu:mimeType>
  </GATECorpusFormat>
  <GATECorpusFormat rdf:ID="GATETransientCorpus">
    <cu:humanReadableFormat xml:lang="en">Transient GATE Corpus</cu:humanReadableFormat>
    <cu:mimeType xml:lang="en">none</cu:mimeType>
  </GATECorpusFormat>
  <GATEPipelineFormat rdf:ID="GATEPipeline_Format">
    <cu:humanReadableFormat xml:lang="en">Non-corpus GATE pipeline</cu:humanReadableFormat>
    <cu:mimeType xml:lang="en">none</cu:mimeType>
  </GATEPipelineFormat>
  <GATE_RTParameterFormat rdf:ID="Standard_GATE_RTP_Format">
    <cu:humanReadableFormat xml:lang="en">GATE Runtime Parameter</cu:humanReadableFormat>
    <cu:mimeType xml:lang="en">none</cu:mimeType>
  </GATE_RTParameterFormat>
  <GATEPipelineFormat rdf:ID="GATEConditionalCorpusPipeline_Format">
    <cu:mimeType xml:lang="en">none</cu:mimeType>
    <cu:humanReadableFormat xml:lang="en">GATE Conditional Corpus Pipeline</cu:humanReadableFormat>
  </GATEPipelineFormat>
  <GATEAnnotationFormat rdf:ID="Standard_GATEAnnotation_Format">
    <cu:humanReadableFormat xml:lang="en">GATE Annotation</cu:humanReadableFormat>
    <cu:mimeType xml:lang="en">none</cu:mimeType>
  </GATEAnnotationFormat>
  <GATEPipelineFormat rdf:ID="GATECorpusPipeline_Format">
    <cu:mimeType xml:lang="en">none</cu:mimeType>
    <cu:humanReadableFormat xml:lang="en">GATE Corpus Pipeline</cu:humanReadableFormat>
  </GATEPipelineFormat>
</rdf:RDF>

